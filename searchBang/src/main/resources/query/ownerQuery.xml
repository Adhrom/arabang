<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="owner">
	<insert id="insertOwner">
		insert into owner_reg values (#{ownerEmail},#{ownerName},#{ownerPw},#{ownerPhone},'n')
	</insert>
	
	<select id="idCheckOwner" parameterType="java.lang.String" resultType="java.lang.Integer">
		select count(*)
		from OWNER_REG where OWNEREMAIL = #{id}
	</select>
</mapper>